<% layout('layouts/boilerplate') %>


<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger my-5" role="alert">
            <h4 class="alert-heading"> <%= err.message %></h4>
            
            <hr>
            <h4>For Devs:</h4>
            <h5>Error Status: <%= err.statusCode %> </h5>
            <p> <%= err.stack %> </p>

        </div>
    </div>
</div>